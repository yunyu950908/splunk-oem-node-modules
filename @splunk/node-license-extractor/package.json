{
  "name": "@splunk/node-license-extractor",
  "version": "2.2.0",
  "description": "A license extractor for node_modules.",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Splunk",
  "scripts": {
    "build": "babel src -d lib",
    "generate:licenses": "node lib/extract",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:ci": "yarn run lint -- -f junit -o test-reports/lint-results.xml",
    "start": "babel src -d lib --watch"
  },
  "main": "lib/extract.js",
  "bin": {
    "splunk-node-license-extractor": "bin/splunk-node-license-extractor"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "chalk": "^2.3.0",
    "execa": "^0.8.0",
    "listr": "^0.13.0",
    "lodash": "^4.17.4",
    "semver": "^5.4.1"
  },
  "devDependencies": {
    "@splunk/babel-preset": "^1.2.0",
    "@splunk/eslint-config": "^1.2.0",
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-add-header-comment": "^1.0.3",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.2.0"
  },
  "engines": {
    "node": ">=6"
  }
}

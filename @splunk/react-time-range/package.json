{
  "name": "@splunk/react-time-range",
  "version": "2.0.0",
  "description": "A Splunk time range picker built with React.",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Splunk",
  "scripts": {
    "build": "NODE_ENV=production webpack --bail",
    "build:ci": "yarn build && yarn docs",
    "docs": "NODE_ENV=production webpack --config webpack.docs.config.js",
    "docs:start": "webpack-dev-server --config webpack.docs.config.js --watch",
    "eslint": "eslint src --ext \".js,.jsx\"",
    "eslint:ci": "eslint src --ext \".js,.jsx\" -f junit -o test-reports/lint-results.xml",
    "eslint:fix": "eslint src --ext \".js, .jsx\" --fix",
    "lint": "yarn run eslint && yarn run stylelint",
    "lint:ci": "yarn run eslint:ci && yarn run stylelint",
    "publish:ci": "cicd-publish-package && cicd-publish-docs",
    "start": "NODE_ENV=development webpack --watch",
    "stylelint": "stylelint \"src/**/*.{js,jsx}\" --config stylelint.config.js",
    "test": "splunk-karma-unit-test-runner start karma.conf.js --single-run",
    "test:ci": "yarn run test -- --reporters mocha,junit --junitDirectory test-reports --junitFilename unit-results.xml --no-colors",
    "test:watch": "splunk-karma-unit-test-runner start karma.conf.js",
    "test:functional": "splunk-wdio-functional-test-runner functional.local.conf.js",
    "test:functional:ci": "splunk-wdio-functional-test-runner functional.ci.conf.js",
    "test:functional:cloud": "splunk-wdio-functional-test-runner functional.cloud.conf.js",
    "test:splunktional:build": "webpack --config test-app/splunktional.webpack.config.js --bail",
    "test:splunktional": "yarn run test:splunktional:build && splunk-wdio-functional-test-runner splunktional.local.conf.js",
    "test:splunktional:ci": "yarn run test:splunktional:build && SPLUNKTIONAL_ORCA_PYTHON=true splunk-wdio-functional-test-runner splunktional.ci.conf.js",
    "test:splunktional:cloud": "yarn run test:splunktional:build && splunk-wdio-functional-test-runner splunktional.cloud.conf.js"
  },
  "dependencies": {
    "@splunk/moment": "^0.4.0",
    "@splunk/react-icons": "^2.0.0",
    "@splunk/splunk-utils": "^1.2.0",
    "@splunk/themes": "^0.3.0",
    "@splunk/time-range-utils": "^1.2.0",
    "@splunk/ui-utils": "^1.1.0",
    "lodash": "^4.17.4"
  },
  "peerDependencies": {
    "@splunk/react-ui": "^2",
    "react": "^16.3",
    "react-dom": "^16.3",
    "styled-components": "^3"
  },
  "devDependencies": {
    "@splunk/babel-preset": "^1.2.0",
    "@splunk/cicd-tools": "^0.0.15",
    "@splunk/docs-gen": "^0.5.0",
    "@splunk/eslint-config": "^1.2.0",
    "@splunk/karma-unit-test-runner": "^3.0.0",
    "@splunk/node-license-extractor": "^2.2.0",
    "@splunk/react-docs": "^0.4.0",
    "@splunk/react-ui": "^2.0.0",
    "@splunk/stylelint-config": "^2.0.0",
    "@splunk/test-runner-utils": "^0.4.0",
    "@splunk/wdio-functional-test-runner": "^7.0.0",
    "@splunk/webpack-configs": "^2.0.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.4",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.5.1",
    "core-js": "^2.5.1",
    "docgen-loader": "^1.3.4",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.2.1",
    "file-loader": "^0.10.0",
    "fs-readdir-recursive": "^1.0.0",
    "html-webpack-plugin": "^4.0.0-alpha.2",
    "raw-loader": "^0.5.1",
    "react": "^16.5.0",
    "react-docgen": "^2.20.1",
    "react-dom": "^16.5.0",
    "react-test-renderer": "^16.5.0",
    "sinon": "^2.3.4",
    "styled-components": "^3.4.5",
    "stylelint": "^9.5.0",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7",
    "webpack-merge": "^4.1.4",
    "whatwg-fetch": "^2.0.3"
  },
  "engines": {
    "node": ">=6"
  }
}
